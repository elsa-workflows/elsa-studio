@using Elsa.Studio.Contracts
<MudIconButton Icon="@Icons.Material.Outlined.Logout" Color="Color.Inherit" OnClick="HandleLogout" Title="Logout"/>

@code {
    [Inject] private ILogoutService LogoutService { get; set; } = null!;

    private async Task HandleLogout()
    {
        await LogoutService.LogoutAsync();
    }
}