<MudLink Typo="Typo.body2" Href="@Href">@DisplayText</MudLink>

@code {
    /// <summary>
    /// The text to display in the link.
    /// </summary>
    [Parameter] public string? DisplayText { get; set; }

    /// <summary>
    /// The URL to navigate to.
    /// </summary>
    [Parameter] public string? Href { get; set; }
}
